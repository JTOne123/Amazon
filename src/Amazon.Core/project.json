{
  "description": "Shared library for Amazon API implementations.",
  "version": "0.3.2",
  "authors": [ "iamcarbon" ],

  "dependencies": {
    "Carbon.Json": "1.0.0",
    "System.Text.Encodings.Web": "4.0.0"
  },

  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50",
      "dependencies": {
        "System.Linq": "4.1.0",
        "System.Runtime": "4.1.0",
        "System.Runtime.Serialization.Primitives": "4.1.1",
        "System.Net.Http": "4.1.0",
        "System.Security.Cryptography.Algorithms": "4.2.0"
      }
    },
    "net461": {
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        },
        "System.Runtime.Serialization": "4.0.0.0",
        "System.Net.Http": "4.0.0.0"
      }
    }
  },

  "packOptions": {
    "tags": [ "aws" ],
    "requireLicenseAcceptance": false,
    "licenseUrl": "https://github.com/carbon/Amazon/blob/master/LICENSE",
    "repository": {
      "type": "git",
