{
  "title": "Amazon S3",
  "description": "A simple wrapper around Amazon's Simple Storage Service.",
  "authors": [ "iamcarbon" ],

  "version": "0.6.0",

  "dependencies": {
    "Amazon.Core": "0.6.0",
    "Carbon.Storage": "1.1.0"
  }, 

  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50",
      "dependencies": {
        "System.Runtime": "4.3.0",
        "System.Xml.XDocument": "4.3.0",
        "System.Xml.XmlSerializer": "4.3.0",
        "System.Security.Cryptography.Algorithms": "4.3.0",
        "System.Net.Http": "4.3.0",
        "System.Collections": "4.3.0"

      }
    },
    "net461": {
      "frameworkAssemblies": {
        "System.Xml": {
          "type": "build",
          "version": "4.0.0.0"
        },
        "System.Xml.Linq": {
          "type": "build",
          "version": "4.0.0.0"
        }
      }
    }
  },

  "packOptions": {
    "tags": [ "aws", "s3" ],
    "licenseUrl": "https://github.com/carbon/Amazon/blob/master/LICENSE",
    "repository": {
      "type": "git",
      "url": "https://github.com/carbon/Amazon"
    }
  }
}